{
  "$id": "proto.ConditionalOrder.jsonschema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "description": "ConditionalOrder demonstrates conditional required fields using CEL",
  "patternProperties": {
    "^(express_fee)$": {
      "anyOf": [
        {
          "exclusiveMinimum": 0,
          "type": "number"
        },
        {
          "enum": [
            "Infinity"
          ],
          "type": "string"
        },
        {
          "type": "string"
        }
      ],
      "description": "expressFee is required when orderType is EXPRESS"
    },
    "^(order_type)$": {
      "anyOf": [
        {
          "enum": [
            "ORDER_TYPE_STANDARD",
            "ORDER_TYPE_EXPRESS",
            "ORDER_TYPE_PRIORITY"
          ],
          "type": "string"
        },
        {
          "maximum": 3,
          "minimum": 1,
          "type": "integer"
        }
      ],
      "description": "orderType determines if expressFee is required",
      "title": "Order Type"
    }
  },
  "properties": {
    "expressFee": {
      "anyOf": [
        {
          "exclusiveMinimum": 0,
          "type": "number"
        },
        {
          "enum": [
            "Infinity"
          ],
          "type": "string"
        },
        {
          "type": "string"
        }
      ],
      "description": "expressFee is required when orderType is EXPRESS"
    },
    "orderType": {
      "anyOf": [
        {
          "enum": [
            "ORDER_TYPE_STANDARD",
            "ORDER_TYPE_EXPRESS",
            "ORDER_TYPE_PRIORITY"
          ],
          "type": "string"
        },
        {
          "maximum": 3,
          "minimum": 1,
          "type": "integer"
        }
      ],
      "description": "orderType determines if expressFee is required",
      "title": "Order Type"
    }
  },
  "required": [
    "orderType"
  ],
  "title": "Conditional Order",
  "type": "object"
}
