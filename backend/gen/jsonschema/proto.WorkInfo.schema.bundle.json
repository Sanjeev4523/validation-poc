{
  "$defs": {
    "google.protobuf.Timestamp.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "format": "date-time",
      "title": "Timestamp",
      "type": "string"
    },
    "proto.WorkInfo.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "WorkInfo represents work-related information",
      "patternProperties": {
        "^(startDate)$": {
          "$ref": "#/$defs/google.protobuf.Timestamp.schema.json",
          "description": "startDate must be after dateOfBirth (validated in parent)"
        }
      },
      "properties": {
        "department": {
          "description": "department is required",
          "maxLength": 50,
          "minLength": 2,
          "type": "string"
        },
        "salary": {
          "anyOf": [
            {
              "minimum": 0,
              "type": "number"
            },
            {
              "enum": [
                "Infinity"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ],
          "description": "salary must be non-negative"
        },
        "start_date": {
          "$ref": "#/$defs/google.protobuf.Timestamp.schema.json",
          "description": "startDate must be after dateOfBirth (validated in parent)"
        }
      },
      "required": [
        "department",
        "salary",
        "start_date"
      ],
      "title": "Work Info",
      "type": "object"
    }
  },
  "$id": "proto.WorkInfo.schema.bundle.json",
  "$ref": "#/$defs/proto.WorkInfo.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
