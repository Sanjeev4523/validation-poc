{
  "$defs": {
    "google.protobuf.Timestamp.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "format": "date-time",
      "title": "Timestamp",
      "type": "string"
    },
    "proto.PersonalInfo.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "PersonalInfo represents personal information with age validation",
      "patternProperties": {
        "^(date_of_birth)$": {
          "$ref": "#/$defs/google.protobuf.Timestamp.jsonschema.json",
          "description": "dateOfBirth is required"
        }
      },
      "properties": {
        "dateOfBirth": {
          "$ref": "#/$defs/google.protobuf.Timestamp.jsonschema.json",
          "description": "dateOfBirth is required"
        },
        "email": {
          "description": "email must be valid",
          "format": "email",
          "minLength": 1,
          "type": "string"
        },
        "name": {
          "description": "name is required",
          "maxLength": 100,
          "minLength": 2,
          "type": "string"
        },
        "phone": {
          "description": "phone must match pattern",
          "minLength": 1,
          "pattern": "^\\+?[1-9]\\d{1,14}$",
          "type": "string"
        }
      },
      "required": [
        "name",
        "email",
        "phone",
        "dateOfBirth"
      ],
      "title": "Personal Info",
      "type": "object"
    }
  },
  "$id": "proto.PersonalInfo.jsonschema.bundle.json",
  "$ref": "#/$defs/proto.PersonalInfo.jsonschema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
