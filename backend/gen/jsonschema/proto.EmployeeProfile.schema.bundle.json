{
  "$defs": {
    "google.protobuf.Timestamp.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "format": "date-time",
      "title": "Timestamp",
      "type": "string"
    },
    "proto.EmergencyContact.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "EmergencyContact represents emergency contact information",
      "properties": {
        "name": {
          "description": "name is required",
          "maxLength": 100,
          "minLength": 2,
          "type": "string"
        },
        "phone": {
          "default": "",
          "description": "phone is required when relationship is SPOUSE",
          "pattern": "^\\+?[1-9]\\d{1,14}$",
          "type": "string"
        },
        "relationship": {
          "anyOf": [
            {
              "enum": [
                "RELATIONSHIP_SPOUSE",
                "RELATIONSHIP_PARENT",
                "RELATIONSHIP_SIBLING",
                "RELATIONSHIP_FRIEND",
                "RELATIONSHIP_OTHER"
              ],
              "type": "string"
            },
            {
              "maximum": 5,
              "minimum": 1,
              "type": "integer"
            }
          ],
          "description": "relationship is required",
          "title": "Relationship"
        }
      },
      "required": [
        "name",
        "relationship"
      ],
      "title": "Emergency Contact",
      "type": "object"
    },
    "proto.EmployeeProfile.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "EmployeeProfile demonstrates deep nesting with multiple CEL constraints",
      "patternProperties": {
        "^(emergencyContact)$": {
          "$ref": "#/$defs/proto.EmergencyContact.schema.json",
          "description": "emergencyContact is required"
        },
        "^(personalInfo)$": {
          "$ref": "#/$defs/proto.PersonalInfo.schema.json",
          "description": "personalInfo is required"
        },
        "^(workInfo)$": {
          "$ref": "#/$defs/proto.WorkInfo.schema.json",
          "description": "workInfo is required"
        }
      },
      "properties": {
        "emergency_contact": {
          "$ref": "#/$defs/proto.EmergencyContact.schema.json",
          "description": "emergencyContact is required"
        },
        "personal_info": {
          "$ref": "#/$defs/proto.PersonalInfo.schema.json",
          "description": "personalInfo is required"
        },
        "work_info": {
          "$ref": "#/$defs/proto.WorkInfo.schema.json",
          "description": "workInfo is required"
        }
      },
      "required": [
        "personal_info",
        "work_info",
        "emergency_contact"
      ],
      "title": "Employee Profile",
      "type": "object"
    },
    "proto.PersonalInfo.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "PersonalInfo represents personal information with age validation",
      "patternProperties": {
        "^(dateOfBirth)$": {
          "$ref": "#/$defs/google.protobuf.Timestamp.schema.json",
          "description": "dateOfBirth is required"
        }
      },
      "properties": {
        "date_of_birth": {
          "$ref": "#/$defs/google.protobuf.Timestamp.schema.json",
          "description": "dateOfBirth is required"
        },
        "email": {
          "description": "email must be valid",
          "format": "email",
          "minLength": 1,
          "type": "string"
        },
        "name": {
          "description": "name is required",
          "maxLength": 100,
          "minLength": 2,
          "type": "string"
        },
        "phone": {
          "description": "phone must match pattern",
          "minLength": 1,
          "pattern": "^\\+?[1-9]\\d{1,14}$",
          "type": "string"
        }
      },
      "required": [
        "name",
        "email",
        "phone",
        "date_of_birth"
      ],
      "title": "Personal Info",
      "type": "object"
    },
    "proto.WorkInfo.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "WorkInfo represents work-related information",
      "patternProperties": {
        "^(startDate)$": {
          "$ref": "#/$defs/google.protobuf.Timestamp.schema.json",
          "description": "startDate must be after dateOfBirth (validated in parent)"
        }
      },
      "properties": {
        "department": {
          "description": "department is required",
          "maxLength": 50,
          "minLength": 2,
          "type": "string"
        },
        "salary": {
          "anyOf": [
            {
              "minimum": 0,
              "type": "number"
            },
            {
              "enum": [
                "Infinity"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ],
          "description": "salary must be non-negative"
        },
        "start_date": {
          "$ref": "#/$defs/google.protobuf.Timestamp.schema.json",
          "description": "startDate must be after dateOfBirth (validated in parent)"
        }
      },
      "required": [
        "department",
        "salary",
        "start_date"
      ],
      "title": "Work Info",
      "type": "object"
    }
  },
  "$id": "proto.EmployeeProfile.schema.bundle.json",
  "$ref": "#/$defs/proto.EmployeeProfile.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
