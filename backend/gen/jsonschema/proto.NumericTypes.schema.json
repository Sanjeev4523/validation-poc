{
  "$id": "proto.NumericTypes.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "description": "NumericTypes demonstrates various numeric validations",
  "patternProperties": {
    "^(doubleValue)$": {
      "anyOf": [
        {
          "maximum": 100,
          "minimum": -100,
          "type": "number"
        },
        {
          "pattern": "^-?[0-9]+(\\.[0-9]+)?([eE][+-]?[0-9]+)?$",
          "type": "string"
        }
      ],
      "default": 0,
      "description": "double_value must be between -100.0 and 100.0"
    },
    "^(floatValue)$": {
      "anyOf": [
        {
          "maximum": 1,
          "minimum": 0,
          "type": "number"
        },
        {
          "pattern": "^-?[0-9]+(\\.[0-9]+)?([eE][+-]?[0-9]+)?$",
          "type": "string"
        }
      ],
      "default": 0,
      "description": "float_value must be between 0.0 and 1.0"
    },
    "^(int32Value)$": {
      "anyOf": [
        {
          "maximum": 1000,
          "minimum": -1000,
          "type": "integer"
        },
        {
          "pattern": "^-?[0-9]+$",
          "type": "string"
        }
      ],
      "default": 0,
      "description": "int32_value must be between -1000 and 1000"
    },
    "^(int64Value)$": {
      "anyOf": [
        {
          "exclusiveMinimum": 0,
          "type": "integer"
        },
        {
          "pattern": "^-?[0-9]+$",
          "type": "string"
        }
      ],
      "default": 0,
      "description": "int64_value must be positive"
    },
    "^(uint32Value)$": {
      "anyOf": [
        {
          "maximum": 10000,
          "minimum": 0,
          "type": "integer"
        },
        {
          "pattern": "^[0-9]+$",
          "type": "string"
        }
      ],
      "default": 0,
      "description": "uint32_value must be at most 10000"
    },
    "^(uint64Value)$": {
      "anyOf": [
        {
          "minimum": 1,
          "type": "integer"
        },
        {
          "pattern": "^[0-9]+$",
          "type": "string"
        }
      ],
      "default": 0,
      "description": "uint64_value must be at least 1"
    }
  },
  "properties": {
    "double_value": {
      "anyOf": [
        {
          "maximum": 100,
          "minimum": -100,
          "type": "number"
        },
        {
          "pattern": "^-?[0-9]+(\\.[0-9]+)?([eE][+-]?[0-9]+)?$",
          "type": "string"
        }
      ],
      "default": 0,
      "description": "double_value must be between -100.0 and 100.0"
    },
    "float_value": {
      "anyOf": [
        {
          "maximum": 1,
          "minimum": 0,
          "type": "number"
        },
        {
          "pattern": "^-?[0-9]+(\\.[0-9]+)?([eE][+-]?[0-9]+)?$",
          "type": "string"
        }
      ],
      "default": 0,
      "description": "float_value must be between 0.0 and 1.0"
    },
    "int32_value": {
      "anyOf": [
        {
          "maximum": 1000,
          "minimum": -1000,
          "type": "integer"
        },
        {
          "pattern": "^-?[0-9]+$",
          "type": "string"
        }
      ],
      "default": 0,
      "description": "int32_value must be between -1000 and 1000"
    },
    "int64_value": {
      "anyOf": [
        {
          "exclusiveMinimum": 0,
          "type": "integer"
        },
        {
          "pattern": "^-?[0-9]+$",
          "type": "string"
        }
      ],
      "default": 0,
      "description": "int64_value must be positive"
    },
    "uint32_value": {
      "anyOf": [
        {
          "maximum": 10000,
          "minimum": 0,
          "type": "integer"
        },
        {
          "pattern": "^[0-9]+$",
          "type": "string"
        }
      ],
      "default": 0,
      "description": "uint32_value must be at most 10000"
    },
    "uint64_value": {
      "anyOf": [
        {
          "minimum": 1,
          "type": "integer"
        },
        {
          "pattern": "^[0-9]+$",
          "type": "string"
        }
      ],
      "default": 0,
      "description": "uint64_value must be at least 1"
    }
  },
  "title": "Numeric Types",
  "type": "object"
}
